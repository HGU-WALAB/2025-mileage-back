<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.csee.swplus.mileage.profile.mapper.ProfileAwardMapper">
    <select id="countAwardsByStudentId" resultType="int">
        SELECT COUNT(*)
        FROM _sw_mileage_award
        WHERE snum = #{studentId}
    </select>
</mapper>